{
  "private": true,
  "main": "index.js",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "turbo run build --filter=forge-inspector-playground",
    "build:lib": "turbo run build --filter=forge-inspector",
    "dev": "turbo run dev --filter=forge-inspector-playground",
    "start": "pnpm --filter forge-inspector-playground start",
    "lint": "turbo run lint --filter=forge-inspector",
    "release": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "eslint": "8.57.1",
    "turbo": "^2.6.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.12",
      "@types/node": "22.8.6"
    }
  }
}
